{
  "problemMatcher": [
    {
      "owner": "rpfm_cli_warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^([01]?\\d|2[0-3])?:?([0-5]?\\d)?:?([0-5]?\\d)?\\s*\\[WARN\\]\\s*(.*)$",
          "message": 4
        }
      ]
    },
    {
      "owner": "rpfm_cli_error",
      "pattern": [
        {
          "regexp": "^(error):\\s*(.*)$",
          "severity": 1,
          "message": 2
        }
      ]
    }
  ]
}
